	<queryString language="SQL">
		<![CDATA[SELECT
c.clase_cuenta AS clase, 
c.cod_cuenta AS cod_cuenta, 
c.nom_cuenta AS nom_cuenta, 
COALESCE(si.debito,0) AS si_debito, 
COALESCE(si.credito,0) AS si_credito, 
COALESCE(s.sum_debito,0) AS sum_debito, 
COALESCE(s.sum_credito,0) AS sum_credito, 
COALESCE(s.total_debito,0) AS total_debito, 
COALESCE(s.total_credito,0) AS total_credito
FROM 
cuenta AS c 
LEFT JOIN saldo_inicial_cuenta AS si 
ON c.cod_cuenta = si.cuenta
		 INNER JOIN saldo_cuenta AS s 
		 ON c.cod_cuenta = s.cuenta AND s.anno = $P{Anno} AND s.mes=$P{mes}
WHERE length(c.cod_cuenta)=4]]>
	</queryString>
